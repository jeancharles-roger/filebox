#!/bin/sh
JAVA=java
BASEDIR=`dirname "$0"`
OSGI_FRAMEWORK=$BASEDIR/plugins/org.eclipse.equinox.launcher_1.0.100.v20080509-1800.jar
VMOPTIONS="-Declipse.ignoreApp=false -Dosgi.noShutdown=false -XstartOnFirstThread"
OPTIONS="-application org.kawane.filebox.ui.filebox"
exec $JAVA $VMOPTIONS -jar "$OSGI_FRAMEWORK" $OPTIONS
